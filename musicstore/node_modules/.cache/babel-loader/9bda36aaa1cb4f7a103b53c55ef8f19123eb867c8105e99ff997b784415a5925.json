{"ast":null,"code":"import getToken from '@/api/login';\nimport { mapMutations } from 'vuex';\n// import {mapState} from 'vuex'\n\nexport default {\n  name: 'LoginName',\n  data() {\n    return {\n      form: {\n        name: '',\n        pwd: '',\n        region: '',\n        delivery: false,\n        type: [],\n        resource: '',\n        desc: ''\n      },\n      formLabelWidth: '90px'\n    };\n  },\n  methods: {\n    ...mapMutations('user', ['changeStatus']),\n    addUser() {\n      let check = localStorage.getItem(this.form.name) || '';\n      if (check) {\n        this.$message({\n          showClose: true,\n          message: '该用户已注册，请登录',\n          type: 'error'\n        });\n        return 0;\n      }\n      localStorage.setItem(this.form.name, this.form.pwd);\n      this.$message({\n        showClose: true,\n        message: '恭喜你，注册成功',\n        type: 'success'\n      });\n    },\n    success() {\n      this.visible = false;\n      this.$store.state.user.login = true;\n      // 需要将状态存到浏览器，不然刷新会回到未登录状态\n      sessionStorage.setItem('login', true);\n      sessionStorage.setItem('token', this.$store.state.user.token);\n      this.$message({\n        showClose: true,\n        message: '登录成功',\n        type: 'success'\n      });\n    },\n    errorPwd() {\n      this.$message({\n        showClose: true,\n        message: '用户名或密码错误',\n        type: 'error'\n      });\n    },\n    async login() {\n      await getToken(this.form).then(res => {\n        this.$store.state.user.token = res.data.access_token;\n      }).catch(err => {\n        console.log(err);\n      });\n      console.log(this.$store.state.user.token);\n      // let checkuser = localStorage.getItem(this.form.name) || ''\n      // this.$message({\n      //   showClose: true,\n      //   message: '查无此用户',\n      //   type: 'warning'\n      // });\n      Boolean(this.$store.state.user.token) == true ? this.success() : this.errorPwd();\n    }\n  },\n  computed: {\n    visible: {\n      get() {\n        //开启了模块化命名\n        return this.$store.state.user.status;\n      },\n      set(val) {\n        this.changeStatus(val);\n      }\n    }\n  },\n  watch: {\n    form: {\n      deep: true,\n      handler(val) {\n        this.form.name = val.name;\n        this.form.pwd = val.pwd;\n      }\n    }\n  }\n};","map":{"version":3,"names":["getToken","mapMutations","name","data","form","pwd","region","delivery","type","resource","desc","formLabelWidth","methods","addUser","check","localStorage","getItem","$message","showClose","message","setItem","success","visible","$store","state","user","login","sessionStorage","token","errorPwd","then","res","access_token","catch","err","console","log","Boolean","computed","get","status","set","val","changeStatus","watch","deep","handler"],"sources":["src/login/login.vue"],"sourcesContent":["<template>\r\n  <!-- 登录的页面 -->\r\n    <div>\r\n        <el-dialog title=\"CloudMusicStore-login\" :visible.sync=\"visible\">\r\n          <el-form :model=\"form\" :style=\"{'text-align':'left'}\" >\r\n              <el-form-item label=\"用户名\" :label-width=\"formLabelWidth\">\r\n                  <el-input v-model=\"form.name\" autofocus=\"true\" autocomplete=\"off\" :style=\"{width:'90%'}\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item label=\"密码\" :label-width=\"formLabelWidth\">\r\n                  <el-input v-model=\"form.pwd\" show-password autocomplete=\"off\" :style=\"{width:'90%'}\"></el-input>\r\n              </el-form-item>\r\n          </el-form>\r\n          <div slot=\"footer\" class=\"dialog-footer\" :style=\"{'text-align':'center'}\">\r\n            <el-button @click=\"addUser\" >注 册</el-button>  \r\n            <el-button type=\"primary\" @click=\"login\">登 录</el-button>\r\n          </div>\r\n        </el-dialog>  \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport getToken from '@/api/login';\r\nimport { mapMutations } from 'vuex'\r\n// import {mapState} from 'vuex'\r\n\r\nexport default {\r\n    name:'LoginName',\r\n    data() {\r\n      return {\r\n        form: {\r\n          name:'',\r\n          pwd:'',\r\n          region: '',\r\n          delivery: false,\r\n          type: [],\r\n          resource: '',\r\n          desc: ''\r\n        },\r\n        formLabelWidth: '90px',\r\n      };\r\n    },\r\n    methods:{\r\n      ...mapMutations('user',['changeStatus']),\r\n      addUser(){\r\n        let check = localStorage.getItem(this.form.name) || ''\r\n        if(check){\r\n          this.$message({\r\n          showClose: true,\r\n          message: '该用户已注册，请登录',\r\n          type: 'error'\r\n          });\r\n          return 0 \r\n        }\r\n        localStorage.setItem(this.form.name, this.form.pwd)\r\n        this.$message({\r\n          showClose: true,\r\n          message: '恭喜你，注册成功',\r\n          type: 'success'\r\n        });\r\n      },\r\n      success(){\r\n        this.visible = false\r\n        this.$store.state.user.login = true\r\n        // 需要将状态存到浏览器，不然刷新会回到未登录状态\r\n        sessionStorage.setItem('login', true)\r\n        sessionStorage.setItem('token',this.$store.state.user.token)\r\n        this.$message({\r\n          showClose: true,\r\n          message: '登录成功',\r\n          type: 'success'\r\n        });\r\n\r\n        \r\n      },\r\n      errorPwd(){\r\n        this.$message({\r\n          showClose: true,\r\n          message: '用户名或密码错误',\r\n          type: 'error'\r\n        });\r\n      },\r\n      async login(){\r\n        await getToken(this.form).then(res=>{\r\n            this.$store.state.user.token = res.data.access_token\r\n        }).catch(err=>{\r\n          console.log(err);\r\n        })\r\n        console.log(this.$store.state.user.token);\r\n        // let checkuser = localStorage.getItem(this.form.name) || ''\r\n        // this.$message({\r\n        //   showClose: true,\r\n        //   message: '查无此用户',\r\n        //   type: 'warning'\r\n        // });\r\n        Boolean(this.$store.state.user.token) == true ? this.success() : this.errorPwd()\r\n      }\r\n    },\r\n    computed:{\r\n      visible:{\r\n        get(){\r\n          //开启了模块化命名\r\n          return this.$store.state.user.status\r\n        },\r\n        set(val){\r\n          this.changeStatus(val)\r\n        }\r\n      },\r\n      \r\n    },\r\n    watch:{\r\n      form:{\r\n        deep:true,\r\n        handler(val){\r\n          this.form.name = val.name\r\n          this.form.pwd = val.pwd\r\n        }\r\n      }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAqBA,OAAAA,QAAA;AACA,SAAAC,YAAA;AACA;;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAF,IAAA;QACAG,GAAA;QACAC,MAAA;QACAC,QAAA;QACAC,IAAA;QACAC,QAAA;QACAC,IAAA;MACA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAX,YAAA;IACAY,QAAA;MACA,IAAAC,KAAA,GAAAC,YAAA,CAAAC,OAAA,MAAAZ,IAAA,CAAAF,IAAA;MACA,IAAAY,KAAA;QACA,KAAAG,QAAA;UACAC,SAAA;UACAC,OAAA;UACAX,IAAA;QACA;QACA;MACA;MACAO,YAAA,CAAAK,OAAA,MAAAhB,IAAA,CAAAF,IAAA,OAAAE,IAAA,CAAAC,GAAA;MACA,KAAAY,QAAA;QACAC,SAAA;QACAC,OAAA;QACAX,IAAA;MACA;IACA;IACAa,QAAA;MACA,KAAAC,OAAA;MACA,KAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,KAAA;MACA;MACAC,cAAA,CAAAP,OAAA;MACAO,cAAA,CAAAP,OAAA,eAAAG,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAG,KAAA;MACA,KAAAX,QAAA;QACAC,SAAA;QACAC,OAAA;QACAX,IAAA;MACA;IAGA;IACAqB,SAAA;MACA,KAAAZ,QAAA;QACAC,SAAA;QACAC,OAAA;QACAX,IAAA;MACA;IACA;IACA,MAAAkB,MAAA;MACA,MAAA1B,QAAA,MAAAI,IAAA,EAAA0B,IAAA,CAAAC,GAAA;QACA,KAAAR,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAG,KAAA,GAAAG,GAAA,CAAA5B,IAAA,CAAA6B,YAAA;MACA,GAAAC,KAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA;MACAC,OAAA,CAAAC,GAAA,MAAAb,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAG,KAAA;MACA;MACA;MACA;MACA;MACA;MACA;MACAS,OAAA,MAAAd,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAG,KAAA,iBAAAP,OAAA,UAAAQ,QAAA;IACA;EACA;EACAS,QAAA;IACAhB,OAAA;MACAiB,IAAA;QACA;QACA,YAAAhB,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAe,MAAA;MACA;MACAC,IAAAC,GAAA;QACA,KAAAC,YAAA,CAAAD,GAAA;MACA;IACA;EAEA;EACAE,KAAA;IACAxC,IAAA;MACAyC,IAAA;MACAC,QAAAJ,GAAA;QACA,KAAAtC,IAAA,CAAAF,IAAA,GAAAwC,GAAA,CAAAxC,IAAA;QA