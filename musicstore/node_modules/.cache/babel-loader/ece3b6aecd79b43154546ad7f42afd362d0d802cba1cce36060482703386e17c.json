{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CountNumber from '../details/Count.vue';\nimport { mapState, mapMutations, mapGetters, mapActions } from 'vuex';\nexport default {\n  name: 'BusName',\n  components: {\n    CountNumber\n  },\n  computed: {\n    ...mapState('home', ['productionData']),\n    ...mapState('cart', ['shoppingCart']),\n    ...mapGetters('cart', ['getTotalPrice', 'checkedShoppingCart'])\n  },\n  methods: {\n    ...mapMutations('cart', ['changeCheck', 'deleteShoppingCart', 'getShoppingCartNumber']),\n    ...mapActions('cart', ['changeAllCheck']),\n    toOrder() {\n      this.$router.push({\n        name: 'order'\n      });\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["CountNumber","mapState","mapMutations","mapGetters","mapActions","name","components","computed","methods","toOrder","$router","push","mounted"],"sources":["src/store_bus/Bus.vue"],"sourcesContent":["<template>\r\n    <!-- 购物车操作页面 -->\r\n    <div>\r\n        <div class=\"goods-list\">\r\n            <div class=\"list-head\">\r\n                <div class=\"col check\"><input type=\"checkbox\" @click=\"changeAllCheck\">全选</div>\r\n                <div class=\"col img\"> &nbsp;</div>\r\n                <div class=\"col name\">商品名称</div>\r\n                <div class=\"col price\">单价</div>\r\n                <div class=\"col num\">数量</div>\r\n                <div class=\"col total\">小计</div>\r\n                <div class=\"col action\">操作</div>\r\n            </div>\r\n            <div class=\"list-body\">\r\n                <div class=\"list-item\" v-for=\"item in shoppingCart\" :key=\"item.id\">\r\n                    <div class=\"item-box\">\r\n                        <div class=\"item-tabel\">\r\n                            <div class=\"item-row\" :style=\"{clear:'both'}\">\r\n                                <div class=\"col check\"><input type=\"checkbox\" :checked=\"item.check\" @click=\"changeCheck(item.id)\">&nbsp;&nbsp;</div>\r\n     