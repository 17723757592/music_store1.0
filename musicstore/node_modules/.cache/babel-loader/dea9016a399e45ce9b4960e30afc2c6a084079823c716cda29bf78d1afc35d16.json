type: 'error'\r\n          });\r\n          return 0 \r\n        }\r\n        localStorage.setItem(this.form.name, this.form.pwd)\r\n        this.$message({\r\n          showClose: true,\r\n          message: '恭喜你，注册成功',\r\n          type: 'success'\r\n        });\r\n      },\r\n      success(){\r\n        this.$message({\r\n          showClose: true,\r\n          message: '登录成功',\r\n          type: 'success'\r\n        });\r\n      },\r\n      errorPwd(){\r\n        this.$message({\r\n          showClose: true,\r\n          message: '密码错误',\r\n          type: 'error'\r\n        });\r\n      },\r\n      login(){\r\n        let check = localStorage.getItem(this.form.name) || ''\r\n        if(check){\r\n          this.form.pwd == check ? this.success() : this.errorPwd()\r\n          // this.visible = false\r\n         return 0 \r\n        }\r\n        this.$message({\r\n          showClose: true,\r\n          message: '查无此用户',\r\n          type: 'warning'\r\n        });\r\n      }\r\n    },\r\n    computed:{\r\n        visible:{\r\n          get(){\r\n            return this.store.state.status\r\n          },\r\n          set(val){\r\n            this.changeStatus(val)\r\n          }\r\n        }\r\n    },\r\n    watch:{\r\n      form:{\r\n        deep:true,\r\n        handler(val){\r\n          this.form.name = val.name\r\n          this.form.pwd = val.pwd\r\n        }\r\n      }\r\n    },\r\n    mounted(){\r\n      console.log(this.visible, this.status);\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAoBA,SAAAA,SAAA;AAEA;EACAC,IAAA;EACAC,KAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAH,IAAA;QACAI,GAAA;QACAC,MAAA;QACAC,QAAA;QACAC,IAAA;QACAC,QAAA;QACAC,IAAA;MACA;MACAC,cAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAZ,SAAA;IACAa,QAAA;MACA,IAAAC,KAAA,GAAAC,YAAA,CAAAC,OAAA,MAAAZ,IAAA,CAAAH,IAAA;MACA,IAAAa,KAAA;QACA,KAAAG,QAAA;UACAC,SAAA;UACAC,OAAA;UACAX,IAAA;QACA;QACA;MACA;MACAO,YAAA,CAAAK,OAAA,MAAAhB,IAAA,CAAAH,IAAA,OAAAG,IAAA,CAAAC,GAAA;MACA,KAAAY,QAAA;QACAC,SAAA;QAC