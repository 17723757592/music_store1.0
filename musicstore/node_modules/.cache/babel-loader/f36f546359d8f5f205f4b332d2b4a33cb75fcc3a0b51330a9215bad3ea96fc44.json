{"ast":null,"code":"import SortTop from '../views/SortTop.vue';\nexport default {\n  name: 'PersonalCenter',\n  props: ['bread'],\n  components: {\n    SortTop\n  },\n  data() {\n    return {\n      tabPosition: 'left',\n      perNav: sessionStorage.getItem(\"report\") || this.bread,\n      // activeName:sessionStorage.getItem(\"report\") || '我的订单',\n      perNavList: [{\n        nav: '我的订单',\n        name: 'perorder'\n      }, {\n        name: 'perdiscount',\n        nav: \"我的优惠券\"\n      }, {\n        name: 'peraddress',\n        nav: \"我的收货地址\"\n      }, {\n        name: 'customersevice',\n        nav: \"在线客服\"\n      }]\n    };\n  },\n  methods: {\n    changeNav(nav, name) {\n      this.perNav = nav, this.$router.replace({\n        name: name\n      }), sessionStorage.setItem(\"report\", nav);\n    }\n  },\n  mounted() {\n    console.log(sessionStorage.getItem('report'), this.bread);\n  }\n};","map":{"version":3,"names":["SortTop","name","props","components","data","tabPosition","perNav","sessionStorage","getItem","bread","perNavList","nav","methods","changeNav","$router","replace","setItem","mounted","console","log"],"sources":["src/user/user.vue"],"sourcesContent":["<template>\r\n    <!-- 登陆后个人中心页面 -->\r\n    <div class=\"per-center\">\r\n        <SortTop :bread=\"perNav\">\r\n        </SortTop>\r\n        <div class=\"per-navlist\">\r\n            <el-tabs :tab-position=\"tabPosition\" style=\"height: 160px;\" label=\"item.nav\">\r\n                <el-tab-pane v-for=\"(item,index) in perNavList\" :key=\"index\">\r\n                    <div slot=\"label\" @click=\"changeNav(item.nav,item.name)\">\r\n                        {{item.nav}}\r\n                    </div>\r\n                    <router-link :to=\"{name:'perorder'}\"></router-link>\r\n                </el-tab-pane>\r\n            </el-tabs>\r\n        </div>\r\n        <div class=\"childbox\">\r\n            <router-view></router-view>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\