{"ast":null,"code":"import SortTop from '../views/SortTop.vue';\nexport default {\n  name: 'PersonalCenter',\n  props: ['bread'],\n  components: {\n    SortTop\n  },\n  data() {\n    return {\n      tabPosition: 'left',\n      number: 'sessionStorage.getItem(\"report\")' || 0,\n      perNav: sessionStorage.getItem(\"report\") || this.bread,\n      // activeName:sessionStorage.getItem(\"report\") || '我的订单',\n      perNavList: [{\n        nav: '我的订单',\n        name: 'perorder',\n        number: 0\n      }, {\n        name: 'perdiscount',\n        nav: \"我的优惠券\",\n        number: 1\n      }, {\n        name: 'peraddress',\n        nav: \"我的收货地址\",\n        number: 2\n      }, {\n        name: 'customersevice',\n        nav: \"在线客服\",\n        number: 3\n      }]\n    };\n  },\n  methods: {\n    changeNav(nav, name, number) {\n      this.perNav = nav, this.$router.replace({\n        name: name\n      }), sessionStorage.setItem(\"report\", nav);\n      //将所需要展示的tab标签的index进行记录\n      sessionStorage.setItem(\"index\", number);\n    }\n  },\n  mounted() {\n    console.log(this.number, this.perNav);\n  }\n};","map":{"version":3,"names":["SortTop","name","props","components","data","tabPosition","number","perNav","sessionStorage","getItem","bread","perNavList","nav","methods","changeNav","$router","replace","setItem","mounted","console","log"],"sources":["src/user/user.vue"],"sourcesContent":["<template>\r\n    <!-- 登陆后个人中心页面 -->\r\n    <div class=\"per-center\">\r\n        <SortTop :bread=\"perNav\">\r\n        </SortTop>\r\n        <div class=\"per-navlist\">\r\n            <el-tabs :tab-position=\"tabPosition\" style=\"height: 160px;\" v-model=\"number\">\r\n                <el-tab-pane v-for=\"(item,index) in perNavList\" :key=\"index\" label=\"item.nav\" >\r\n                    <div slot=\"label\" @click=\"changeNav(item.nav,item.name)\">\r\n                        {{item.nav}}\r\n                    </div>\r\n                    <router-link :to=\"{name:'perorder'}\"></router-link>\r\n                </el-tab-pane>\r\n            </el-tabs>\r\n        </div>\r\n        <div class=\"childbox\">\r\n            <router-view></router-view>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport SortTop from '../views/SortTop.vue'\r\nexport default {\r\n    name:'PersonalCenter',\r\n    props:['bread'],\r\n    components:{SortTop},\r\n    data(){\r\n      return {\r\n        tabPosition: 'left',\r\n        number:'sessionStorage.getItem(\"report\")' || 0,\r\n        perNav:sessionStorage.getItem(\"report\") || this.bread,\r\n        // activeName:sessionStorage.getItem(\"report\") || '我的订单',\r\n        perNavList:[\r\n        {nav:'我的订单',name:'perorder',number:0},\r\n        {name:'perdiscount',nav:\"我的优惠券\",number:1},\r\n        {name:'peraddress',nav:\"我的收货地址\",number:2},\r\n        {name:'customersevice',nav:\"在线客服\",number:3}]\r\n      };\r\n    },\r\n    methods:{\r\n        changeNav(nav,name,number){\r\n            this.perNav = nav,\r\n            this.$router.replace({\r\n                name:name,\r\n            }),\r\n            sessionStorage.setItem(\"report\",nav);\r\n            //将所需要展示的tab标签的index进行记录\r\n            sessionStorage.setItem(\"index\",number);\r\n        },\r\n    },\r\n    mounted(){\r\n        console.log(this.number, this.perNav);\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.childbox{\r\n    height: 480px;\r\n}\r\n.per-center{\r\n    width: 1100px;\r\n    margin: 0 auto;\r\n    overflow: hidden;\r\n    background: #fff;\r\n    padding-bottom: 160px;\r\n    .per-navlist{\r\n        float: left;\r\n    }\r\n}\r\n</style>"],"mappings":"AAsBA,OAAAA,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,KAAA;IACA;MACAC,WAAA;MACAC,MAAA;MACAC,MAAA,EAAAC,cAAA,CAAAC,OAAA,mBAAAC,KAAA;MACA;MACAC,UAAA,GACA;QAAAC,GAAA;QAAAX,IAAA;QAAAK,MAAA;MAAA,GACA;QAAAL,IAAA;QAAAW,GAAA;QAAAN,MAAA;MAAA,GACA;QAAAL,IAAA;QAAAW,GAAA;QAAAN,MAAA;MAAA,GACA;QAAAL,IAAA;QAAAW,GAAA;QAAAN,MAAA;MA