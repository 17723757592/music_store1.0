{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport jifen from '../../assets/jifen.png';\nimport getCategory from '@/api/getCategory';\nexport default {\n  name: 'SortTab',\n  data() {\n    return {\n      tabList: [{\n        title: 'IP周边',\n        href: 'https://music.163.com/store/product/variouskind?cid=1008002&title=IP周边',\n        name: 'sort',\n        identifying: 'IPper',\n        ico: 'http://p4.music.126.net/tKMAm5OvR-2lAj7dnEOhsg==/18623527952924939.jpg'\n      }, {\n        title: '数码影音',\n        href: 'store',\n        name: 'sort',\n        identifying: 'DigitalVideo',\n        ico: 'http://p3.music.126.net/PzH4QQKE5R97J9f2V-SvqQ==/18585045045959929.jpg'\n      }, {\n        title: '热销爆品',\n        href: '/store/product/column?id=55001&title=热销爆品',\n        name: 'earphone',\n        ico: 'http://p3.music.126.net/UKhDHWnEMmoOo27PQmDPbA==/19117208672446378.jpg'\n      }, {\n        title: '云贝商城',\n        href: '',\n        ico: 'https://s2.music.126.net/store/web/img/jifen.png'\n      }]\n    };\n  },\n  methods: {\n    turnTo() {\n      this.$router.push({\n        name: 'sort',\n        query: {\n          id: this.tabList.id\n        }\n      });\n    }\n  },\n  async created() {\n    await getCategory().then(res => {\n      this.tabList = res.data.data;\n    });\n  },\n  mounted() {\n    console.log(this.tabList);\n  }\n};","map":{"version":3,"names":["jifen","getCategory","name","data","tabList","title","href","identifying","ico","methods","turnTo","$router","push","query","id","created","then","res","mounted","console","log"],"sources":["src/components/home/SortTab.vue"],"sourcesContent":["<template>\r\n    <!-- ip周边，数码影音等导航区 -->\r\n    <div>\r\n        <div class=\"gtab\">\r\n            <div class=\"hometab\">\r\n                <ul>\r\n                    <li class=\"tab\" v-for=\"(tab,index) in tabList\" :key=\"index\">\r\n                        <!-- <a :href=\"tab.href\"> -->\r\n                        \r\n                        <router-link :to=\"{name:tab.name,params:{bread:tab.title}}\">\r\n                            <img :src=\"tab.ico\" alt=\"tab-icos\">\r\n                            <span>{{tab.title}}</span>\r\n                        </router-link>\r\n                            \r\n                        <em class=\"line\" v-if=\"index !== tabList.length-1\"></em>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport jifen from '../../assets/jifen.png'\r\nimport getCategory from '@/api/getCategory';\r\nexport default {\r\n    name:'SortTab',\r\n    data(){\r\n        return{\r\n            tabList:[\r\n            {title:'IP周边',href:'https://music.163.com/store/product/variouskind?cid=1008002&title=IP周边',name:'sort',identifying:'IPper',\r\n            ico:'http://p4.music.126.net/tKMAm5OvR-2lAj7dnEOhsg==/18623527952924939.jpg'},\r\n            {title:'数码影音',href:'store',name:'sort',identifying:'DigitalVideo',            \r\n            ico:'http://p3.music.126.net/PzH4QQKE5R97J9f2V-SvqQ==/18585045045959929.jpg'},\r\n            {title:'热销爆品',href:'/store/product/column?id=55001&title=热销爆品',name:'earphone',\r\n            ico:'http://p3.music.126.net/UKhDHWnEMmoOo27PQmDPbA==/19117208672446378.jpg'},\r\n            {title:'云贝商城',href:'',ico:'https://s2.music.126.net/store/web/img/jifen.png'}\r\n        ]\r\n        }\r\n    \r\n    },\r\n    methods:{\r\n        turnTo(){\r\n            this.$router.push({\r\n                name:'sort',\r\n                query:{\r\n                    id:this.tabList.id,\r\n                }\r\n            })\r\n        }\r\n    },\r\n    async created(){\r\n        await getCategory().then(res=>{\r\n            this.tabList = res.data.data\r\n        })\r\n    },\r\n    mounted(){\r\n        console.log(this.tabList);\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.gtab{\r\n    width: 100%;\r\n    height: 80px;\r\n    position: relative;\r\n    border-bottom: 1px solid rgba(0, 0, 0, 0.1);\r\n}\r\n.hometab{\r\n    width: 1100px;\r\n    margin: 0 auto;\r\n}\r\n.hometab ul{\r\n    width: 100%;\r\n    height: 80px;\r\n}\r\n/* .hometab ::after{\r\n    clear: both;\r\n    content: \".\";\r\n    display: block;\r\n    height: 0;\r\n    visibility: hidden;\r\n} */\r\n.hometab ul li{\r\n    list-style: none;\r\n} \r\n.hometab ul li a{\r\n    float: left;\r\n    width: 250px;\r\n    padding: 0 10px;\r\n    height: 80px;\r\n    text-align: center;\r\n    font-size: 0;\r\n}\r\n.line{\r\n    display: inline-block;\r\n    height: 48px;\r\n    width: 1px;\r\n    margin: 16px 0 16px 0;\r\n    background-color: #000000;\r\n    opacity: 0.1;\r\n}\r\n.hometab ul li img{\r\n    display: inline-block;\r\n    width: 48px;\r\n    height: 48px;\r\n    vertical-align: top;\r\n    padding-top: 16px;\r\n    border: 0;\r\n}\r\n.tab{\r\n    float: left;\r\n    width: 275px;\r\n    height: 80px\r\n}\r\n.tab span{\r\n    display: inline-block;\r\n    max-width: 190px;\r\n    height: 80px;\r\n    padding-left: 12px;\r\n    line-height: 80px;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n    color: #333333;\r\n}\r\n</style>"],"mappings":";AAuBA,OAAAA,KAAA;AACA,OAAAC,WAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA,GACA;QAAAC,KAAA;QAAAC,IAAA;QAAAJ,IAAA;QAAAK,WAAA;QACAC,GAAA;MAAA,GACA;QAAAH,KAAA;QAAAC,IAAA;QAAAJ,IAAA;QAAAK,WAAA;QACAC,GAAA;MAAA,GACA;QAAAH,KAAA;QAAAC,IAAA;QAAAJ,IAAA;QACAM,GAAA;MAAA,GACA;QAAAH,KAAA;QAAAC,IAAA;QAAAE,GAAA;MAAA;IAEA;EAEA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAV,IAAA;QACAW,KAAA;UACAC,EAAA,OAAAV,OAAA,CAAAU;QACA;MACA;IACA;EACA;EACA,MAAAC,QAAA;IACA,MAAAd,WAAA,GAAAe,IAAA,CAAAC,GAAA;MACA,KAAAb,OAAA,GAAAa,GAAA,CAAAd,IAAA,CAAAA,IAAA;IACA;EACA;EACAe,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAhB,OAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}